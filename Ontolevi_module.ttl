#################################################################

###  http://prismstandard.org/namespaces/basic/2.0/alternateTitle
<http://prismstandard.org/namespaces/basic/2.0/alternateTitle> rdf:type owl:DatatypeProperty ;
                                                               rdfs:subPropertyOf dcterms:title ;
                                                               rdfs:comment "An alternative title for a resource."@en ;
                                                               rdfs:label "alternate title"@en .


###  http://prismstandard.org/namespaces/basic/2.0/doi
<http://prismstandard.org/namespaces/basic/2.0/doi> rdf:type owl:DatatypeProperty ;
                                                    rdfs:subPropertyOf dcterms:identifier ;
                                                    rdf:type owl:FunctionalProperty ;
                                                    rdfs:domain frbr:Endeavour ;
                                                    rdfs:comment "A Digital Object Identifier for a serial publication item, a dataset or some other entity."@en ;
                                                    rdfs:isDefinedBy <http://www.doi.org/> ;
                                                    rdfs:label "has DOI"@en ,
                                                               "has Digital Object Identifier"@en .


###  http://prismstandard.org/namespaces/basic/2.0/isbn
<http://prismstandard.org/namespaces/basic/2.0/isbn> rdf:type owl:DatatypeProperty ;
                                                     rdfs:subPropertyOf dcterms:identifier ;
                                                     rdf:type owl:FunctionalProperty ;
                                                     rdfs:domain frbr:Endeavour ;
                                                     rdfs:comment "The International Standard Book Number, ISBN, used to identify published books or, more rarely, journal issues."@en ;
                                                     rdfs:label "has ISBN"@en ,
                                                                "has International Standard Book Number"@en .


###  http://prismstandard.org/namespaces/basic/2.0/issueIdentifier
<http://prismstandard.org/namespaces/basic/2.0/issueIdentifier> rdf:type owl:DatatypeProperty ,
                                                                         owl:FunctionalProperty ;
                                                                rdfs:domain frbr:Endeavour ;
                                                                rdfs:comment "An identifier used to identify a specific issue of a magazine, a journal or other resource."@en ;
                                                                rdfs:label "has issue identifier"@en .


###  http://prismstandard.org/namespaces/basic/2.0/keyword
<http://prismstandard.org/namespaces/basic/2.0/keyword> rdf:type owl:DatatypeProperty ;
                                                        rdfs:subPropertyOf owl:topDataProperty ;
                                                        rdfs:domain frbr:Endeavour ;
                                                        rdfs:comment "A property used to tag a resource with a keyword that is likely to be used in search queries."@en ;
                                                        rdfs:label "has keyword"@en .


###  http://prismstandard.org/namespaces/basic/2.0/pageRange
<http://prismstandard.org/namespaces/basic/2.0/pageRange> rdf:type owl:DatatypeProperty ;
                                                          rdfs:subPropertyOf owl:topDataProperty ;
                                                          rdf:type owl:FunctionalProperty ;
                                                          rdfs:domain frbr:Endeavour ;
                                                          rdfs:comment "Identifies the page range of an entity such as a journal article."@en ;
                                                          rdfs:label "has page range"@en .


###  http://prismstandard.org/namespaces/basic/2.0/publicationDate
<http://prismstandard.org/namespaces/basic/2.0/publicationDate> rdf:type owl:DatatypeProperty ;
                                                                rdfs:subPropertyOf dcterms:issued ;
                                                                rdf:type owl:FunctionalProperty ;
                                                                rdfs:domain frbr:Endeavour ;
                                                                rdfs:range [ rdf:type rdfs:Datatype ;
                                                                             owl:unionOf ( xsd:date
                                                                                           xsd:gYear
                                                                                           xsd:gYearMonth
                                                                                         )
                                                                           ] ;
                                                                <http://purl.org/dc/elements/1.1/description> """The date on which a document or entity is published.
The date can be expressed in three different formats:
- yyyy-mm-dd (i.e., xsd:date)
- yyyy-mm (i.e., xsd:gYearMonth)
- yyyy (i.e., xsd:gYear)"""@en ;
                                                                rdfs:comment "The date on which a resource is published or disclosed."@en ;
                                                                rdfs:label "has publication date"@en .


###  http://prismstandard.org/namespaces/basic/2.0/volume
<http://prismstandard.org/namespaces/basic/2.0/volume> rdf:type owl:DatatypeProperty ,
                                                                owl:FunctionalProperty ;
                                                       rdfs:domain frbr:Endeavour ;
                                                       rdfs:comment "An identifier for a particular volume of a resource, such as a journal or a multi-volume book."@en ;
                                                       rdfs:label "has volume identifier"@en .


###  http://purl.org/dc/terms/date
dcterms:date rdf:type owl:DatatypeProperty ;
             rdfs:comment """A point or period of time associated with an event in the lifecycle of the resource.

Date may be used to express temporal information at any level of granularity. Recommended best practice is to use an encoding scheme, such as the W3CDTF profile of ISO 8601 [http://www.w3.org/TR/NOTE-datetime]."""@en ;
             rdfs:label "has date"@en .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:DatatypeProperty ;
                   rdfs:comment """A unique and unambiguous reference to the resource within a given context.

Recommended best practice is to identify the resource by means of a string conforming to a formal identification system."""@en ;
                   rdfs:label "has identifier"@en .


###  http://purl.org/dc/terms/issued
dcterms:issued rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf dcterms:date ;
               rdfs:comment "Date of formal issuance of a resource (e.g. a publication or a patent)."@en ;
               rdfs:label "has issue date"@en .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:DatatypeProperty ;
              rdfs:domain frbr:Endeavour ;
              rdfs:range xsd:string ;
              rdfs:comment "A name given to a resource.  Typically, the title will be the name by which the resource is formally known."@en ;
              rdfs:label "has title"@en .


###  http://purl.org/spar/cito/hasCitationCreationDate
cito:hasCitationCreationDate rdf:type owl:DatatypeProperty ;
                             rdfs:domain cito:Citation ;
                             rdfs:range xsd:date ;
                             rdfs:comment "The date on which the citation was created. This has the same numerical value as the publication date of the citing bibliographic resource, but is a property of the citation itself.  When combined with the citation time span, it permits that citation to be located in history."@en ;
                             rdfs:label "has citation creation date"@en .


###  http://purl.org/spar/cito/hasCitationTimeSpan
cito:hasCitationTimeSpan rdf:type owl:DatatypeProperty ;
                         rdfs:domain cito:Citation ;
                         rdfs:range xsd:duration ;
                         rdfs:comment "The temporal characteristic of a citation, namely the interval between the publication date of the cited entity and the publication date of the citing entity. Note that when one or both of the publication dates is given as just 'year', then the citation time span is rounded to the nearest year, and when one or both of the publication dates is given as just 'year and month', then the citation time span is rounded to the nearest month, with the inherent inaccuracies that such rounding involves."@en ;
                         rdfs:label "has citation time span"@en .


###  http://purl.org/spar/fabio/hasAccessDate
fabio:hasAccessDate rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf dcterms:date ;
                    rdfs:domain fabio:Item ;
                    rdfs:range xsd:dateTime ;
                    rdfs:comment "The date on which a particular digital item, such as a PDF or an HTML file, has been accessed by somebody."@en ;
                    rdfs:label "has access date"@en .


###  http://purl.org/spar/fabio/hasElectronicArticleIdentifier
fabio:hasElectronicArticleIdentifier rdf:type owl:DatatypeProperty ;
                                     rdfs:subPropertyOf owl:topDataProperty ;
                                     rdfs:domain frbr:Endeavour ;
                                     rdfs:comment "A local identifier for an article within an electronic (i.e. on line, in HTML format) periodical issue.  Use in preference to prism:startingPage when the article lacks page numbers"@en ;
                                     rdfs:label "has electronic article identifier"@en .


###  http://purl.org/spar/fabio/hasPublicationYear
fabio:hasPublicationYear rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf dcterms:issued ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:range xsd:gYear ;
                         rdfs:comment "The year in which a resource is published."@en ;
                         rdfs:label "has publication year"@en .


###  http://purl.org/spar/fabio/hasSequenceIdentifier
fabio:hasSequenceIdentifier rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdfs:domain frbr:Endeavour ;
                            rdfs:comment "A literal (for example a number or a letter) that identifies the sequence position of a work within a particular context, for example a book in a book series, a chapter in a document, a volume in a journal."@en ;
                            rdfs:label "has number"@en .


###  http://purl.org/spar/fabio/hasShortTitle
fabio:hasShortTitle rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf dcterms:title ;
                    rdfs:domain frbr:Endeavour ;
                    rdfs:range xsd:string ;
                    rdfs:comment "A short version of the title of an entity, typically used to label or refer to a particular entity in an abbreviated form, for example an abbreviated journal title in a reference, or a short title of a document used as the running title in a page header."@en ;
                    rdfs:label "has short title"@en .


###  http://purl.org/spar/fabio/hasSubtitle
fabio:hasSubtitle rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf dcterms:title ;
                  rdfs:domain frbr:Endeavour ;
                  rdfs:range xsd:string ;
                  rdfs:comment "A secondary title that follows the main title of a work."@en ;
                  rdfs:label "has subtitle"@en .


###  http://purl.org/spar/fabio/hasTranslatedSubtitle
fabio:hasTranslatedSubtitle rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf dcterms:title ;
                            rdfs:comment "A version of the subtitle of an entity translated into another language, which may be specified using the object property dcterms:language."@en ;
                            rdfs:label "has translated subtitle"@en .


###  http://purl.org/spar/fabio/hasTranslatedTitle
fabio:hasTranslatedTitle rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf dcterms:title ;
                         rdfs:comment "A version of the title of an entity translated into another language, which may be specified using the object property dcterms:language."@en ;
                         rdfs:label "has translated title"@en .


###  http://purl.org/spar/fabio/hasURL
fabio:hasURL rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf dcterms:identifier ;
             rdfs:domain frbr:Endeavour ;
             rdfs:range xsd:anyURI ;
             rdfs:comment "An identifier, in form of an HTTP Universal Resource Locator (URL), for a particular resource on the World Wide Web."@en ;
             rdfs:label "has URL"@en .


###  http://purl.org/spar/fabio/hasVolumeCount
fabio:hasVolumeCount rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain frbr:Endeavour ;
                     rdfs:range xsd:nonNegativeInteger ;
                     rdfs:comment "The count of the number of volumes a work includes."@en ;
                     rdfs:label "has volume count"@en .


###  https://schema.org/alternateName
sdo:alternateName rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf sdo:name ;
                  rdfs:domain frbr:ResponsibleEntity ;
                  rdfs:range xsd:string .


###  https://schema.org/birthDate
sdo:birthDate rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf dcterms:date ;
              rdfs:domain frbr:Person ;
              rdfs:range xsd:date .


###  https://schema.org/deathDate
sdo:deathDate rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf dcterms:date ;
              rdfs:domain frbr:Person ;
              rdfs:range xsd:date .


###  https://schema.org/name
sdo:name rdf:type owl:DatatypeProperty ;
         rdfs:domain frbr:Place ,
                     frbr:ResponsibleEntity ;
         rdfs:range xsd:string .


###  ontolevi#codiceAcnp
<ontolevi#codiceAcnp> rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf dcterms:identifier ;
                      rdfs:domain frbr:Manifestation ;
                      rdfs:range xsd:string .


###  ontolevi#codiceEditore
<ontolevi#codiceEditore> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf dcterms:identifier ;
                         rdfs:domain dbo:Publisher ;
                         rdfs:range xsd:string .


###  ontolevi#codiceIccu
<ontolevi#codiceIccu> rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf dcterms:identifier ;
                      rdfs:domain sdo:Library ;
                      rdfs:range xsd:string .


###  ontolevi#codiceOpacSbn
<ontolevi#codiceOpacSbn> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf dcterms:identifier ;
                         rdfs:domain frbr:Manifestation ;
                         rdfs:range xsd:string .


###  ontolevi#permalinkAcnp
<ontolevi#permalinkAcnp> rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf fabio:hasURL ;
                         rdfs:domain frbr:Manifestation ;
                         rdfs:range xsd:anyURI .


###  ontolevi#permalinkOpacSbn
<ontolevi#permalinkOpacSbn> rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf fabio:hasURL ;
                            rdfs:domain frbr:Manifestation ;
                            rdfs:range xsd:anyURI .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Genre
dbo:Genre rdf:type owl:Class .


###  http://dbpedia.org/ontology/Publisher
dbo:Publisher rdf:type owl:Class ;
              rdfs:subClassOf frbr:CorporateBody .


###  http://purl.org/dc/dcmitype/Collection
dcmitype:Collection rdf:type owl:Class .


###  http://purl.org/dc/terms/LinguisticSystem
dcterms:LinguisticSystem rdf:type owl:Class .


###  http://purl.org/spar/cito/Citation
cito:Citation rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty cito:hasCitationCharacterization ;
                                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty cito:hasCitedEntity ;
                                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty cito:hasCitingEntity ;
                                                           owl:cardinality "1"^^xsd:nonNegativeInteger
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment """A citation is a conceptual directional link from a citing entity to a cited entity, created by a human performative act of making a citation, typically instantiated by the inclusion of a bibliographic reference (biro:BibliographicReference) in the reference list of the citing entity, or by the inclusion within the citing entity of a link, in the form of an HTTP Uniform Resource Locator (URL), to a resource on the World Wide Web.

The time span of a citation, i.e. the interval between the publication year of the citing entity and the publication year of the cited entity, can be recorded using the data property cito:hasCitationTimeSpan.

The nature or type of a citation can be characterized by using CiTO object properties, e.g. http://purl.org/spar/cito/citesAsDataSource (definition: The citing entity cites the cited entity as a source of data).  

This CiTO class cito:Citation and its accompanying object properties cito:hasCitingEntity, cito:hasCitedEntity and cito:hasCitationCharacterization can be employed to reify direct citation statements made using the CiTO citation object property cito:cites or one of its sub-properties. 

For example, the following RDF statement

     :paperA cito:extends :paperB .

can be alternatively described as follows

     :thisCitation a cito:Citation ; 
          cito:hasCitingEntity :paperA ;
          cito:hasCitationCharacterization cito:extends ;
          cito:hasCitedEntity :paperB .

This usage involved OWL2 punning, whereby a CiTO object property, such as the aforementioned cito:extends, is used as the object of the OWL assertion.

     :thisCitation cito:hasCitationCharacterization cito:extends .

Using such OWL2 punning (described at http://www.w3.org/TR/2009/WD-owl2-new-features-20090611/#F12:_Punning), the CiTO object property is considered as a proper named individual of the class owl:Thing. 

Such reification of citation acts can be very useful, since it permits one to combine these CiTO properties with other vocabularies, or to handle situations in which none of the citation characterizations available in CiTO are applicable.  

Such situations can be resolved by the creation of a user-defined citation characterization, for example by using the Open Annotation Data Model, as explained at http://semanticpublishing.wordpress.com/2013/07/03/extending-cito-for-open-annotations/."""@en ;
              rdfs:label "citation"@en .


###  http://purl.org/spar/cito/DistantCitation
cito:DistantCitation rdf:type owl:Class ;
                     rdfs:subClassOf cito:Citation ;
                     <http://purl.org/dc/elements/1.1/description> "Derived from the article 'A Small World of Citations? The Influence of Collaboration Networks on Citation Practices' by Matthew L. Wallace, Vincent Larivire and Yves Gingras, published in PLOS One (https://doi.org/10.1371/journal.pone.0033339)."@en ;
                     rdfs:comment "A citation in which the citing and the cited entities have nothing significant in common with one another (for example authors, journal, institutional affiliation, or funding agency) over and beyond their subject matter."@en ;
                     rdfs:label "distant citation"@en .


###  http://purl.org/spar/cito/SelfCitation
cito:SelfCitation rdf:type owl:Class ;
                  rdfs:subClassOf cito:Citation ;
                  rdfs:comment "A citation in which the citing and the cited entities have something significant in common with one another, over and beyond their subject matter, for example authors, journal, institutional affiliation, or funding agency."@en ;
                  rdfs:label "self citation"@en .


###  http://purl.org/spar/fabio/AnalogItem
fabio:AnalogItem rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Item ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty fabio:isStoredOn ;
                                   owl:someValuesFrom fabio:AnalogStorageMedium
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty frbr:exemplarOf ;
                                   owl:someValuesFrom fabio:AnalogManifestation
                                 ] ;
                 <http://purl.org/dc/elements/1.1/description> "An analog item is an exemplar of an analog manifestation only and it is always stored in a storage medium suitable for analog objects, such as paper, vinyl discs and films."@en ;
                 rdfs:comment "A real object that is an exemplar of a fabio:Manifestation, such as a particular copy of the book 'Alice's adventures in Wonderland', that a person may own."@en ;
                 rdfs:label "analog item"@en .


###  http://purl.org/spar/fabio/AnalogManifestation
fabio:AnalogManifestation rdf:type owl:Class ;
                          rdfs:subClassOf fabio:Manifestation ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty frbr:exemplar ;
                                            owl:someValuesFrom fabio:AnalogItem
                                          ] ;
                          rdfs:comment "A manifestation in an analog form."@en ;
                          rdfs:label "analog manifestation"@en .


###  http://purl.org/spar/fabio/AnalogStorageMedium
fabio:AnalogStorageMedium rdf:type owl:Class ;
                          rdfs:subClassOf fabio:StorageMedium ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty fabio:stores ;
                                            owl:allValuesFrom fabio:AnalogItem
                                          ] ;
                          rdfs:comment "A means of storing information in non-digital form, e.g. paper, film (for analogue photographs or movies), magnetic tape (for analogue sound recordings or video recordings) or vinyl disc."@en ;
                          rdfs:label "analog storage medium"@en .


###  http://purl.org/spar/fabio/Anthology
fabio:Anthology rdf:type owl:Class ;
                rdfs:subClassOf fabio:ExpressionCollection ;
                rdfs:comment "A collection of selected literary or scholastics works, for example poems, short stories, plays or research papers."@en ;
                rdfs:label "anthology"@en .


###  http://purl.org/spar/fabio/ArchivalDocument
fabio:ArchivalDocument rdf:type owl:Class ;
                       rdfs:subClassOf fabio:Expression ;
                       rdfs:comment "An archival document is a realization of the content related to an archival record. It can be exemplified as a book, a document, a letter, a database, etc."@en ;
                       rdfs:label "archival document"@en .


###  http://purl.org/spar/fabio/ArchivalDocumentSet
fabio:ArchivalDocumentSet rdf:type owl:Class ;
                          rdfs:subClassOf fabio:ExpressionCollection ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty frbr:part ;
                                            owl:someValuesFrom fabio:ArchivalDocument
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty frbr:part ;
                                            owl:allValuesFrom fabio:ArchivalDocument
                                          ] ;
                          rdfs:comment "A collection of archival document."@en ;
                          rdfs:label "archival document set"@en .


###  http://purl.org/spar/fabio/ArchivalRecord
fabio:ArchivalRecord rdf:type owl:Class ;
                     rdfs:subClassOf fabio:Work ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty frbr:realization ;
                                       owl:allValuesFrom fabio:ArchivalDocument
                                     ] ;
                     rdfs:comment "An archival record connotes a material created or received by a person, family, or organization, public or private, in the conduct of their affairs that is preserved because of the enduring value contained in the information it contains or as evidence of the function and the responsibilities of its creator."@en ;
                     rdfs:label "archival record"@en .


###  http://purl.org/spar/fabio/ArchivalRecordSet
fabio:ArchivalRecordSet rdf:type owl:Class ;
                        rdfs:subClassOf fabio:WorkCollection ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty frbr:part ;
                                          owl:someValuesFrom fabio:ArchivalRecord
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty frbr:part ;
                                          owl:allValuesFrom fabio:ArchivalRecord
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty frbr:realization ;
                                          owl:allValuesFrom fabio:ArchivalDocumentSet
                                        ] ;
                        rdfs:comment "A collection of archival records."@en ;
                        rdfs:label "archival record set"@en .


###  http://purl.org/spar/fabio/Article
fabio:Article rdf:type owl:Class ;
              rdfs:subClassOf fabio:Expression ;
              rdfs:comment "The realization of a piece of writing on a particular topic, usually published within a periodical publication (e.g. journal, magazine and newspaper)."@en ;
              rdfs:label "article"@en .


###  http://purl.org/spar/fabio/ArtisticWork
fabio:ArtisticWork rdf:type owl:Class ;
                   rdfs:subClassOf fabio:Work ;
                   rdfs:comment "It describes any work regarded as art in its widest sense, including works from literature and music, visual art, etc."@en ;
                   rdfs:label "artistic work"@en .


###  http://purl.org/spar/fabio/AudioDocument
fabio:AudioDocument rdf:type owl:Class ;
                    rdfs:subClassOf fabio:Expression ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:realizationOf ;
                                      owl:someValuesFrom fabio:SoundRecording
                                    ] ;
                    rdfs:comment "The realization of a sound recording."@en ;
                    rdfs:label "audio document"@en .


###  http://purl.org/spar/fabio/Biography
fabio:Biography rdf:type owl:Class ;
                rdfs:subClassOf fabio:Work ;
                rdfs:comment "An account of the events, works and achievements, both personal and professional, of a person, either living or dead."@en ;
                rdfs:label "biography"@en .


###  http://purl.org/spar/fabio/Book
fabio:Book rdf:type owl:Class ;
           rdfs:subClassOf fabio:Expression ;
           rdfs:comment "A non-serial document that is complete in one volume or a designated finite number of volumes.  A book published by a publisher is usually  identified by an International Standard Book Number (ISBN), and may be manifested as a physical printed publication on paper bound in a hard or soft cover, or in electronic format as an 'e-book'."@en ;
           rdfs:label "book"@en .


###  http://purl.org/spar/fabio/BookChapter
fabio:BookChapter rdf:type owl:Class ;
                  rdfs:subClassOf fabio:Chapter ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty frbr:partOf ;
                                    owl:someValuesFrom fabio:Book
                                  ] ;
                  rdfs:comment "A defined chapter of a book, usually with a separate title or number."@en ;
                  rdfs:label "book chapter"@en .


###  http://purl.org/spar/fabio/BookReview
fabio:BookReview rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Review ;
                 rdfs:comment "A written review and critical analysis of the content, scope and quality of a book or other monographic work."@en ;
                 rdfs:label "book review"@en .


###  http://purl.org/spar/fabio/BookSeries
fabio:BookSeries rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Series ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty frbr:part ;
                                   owl:someValuesFrom fabio:Book
                                 ] ;
                 rdfs:comment "A sequence of books having certain characteristics in common that are formally identified together as a group - for instance, the books in the Law, Governance and Technology Series published by Springer."@en ;
                 rdfs:label "book series"@en .


###  http://purl.org/spar/fabio/Chapter
fabio:Chapter rdf:type owl:Class ;
              rdfs:subClassOf fabio:Expression ;
              rdfs:comment "A defined document section, forming part of or intended for inclusion within a larger document, usually with its own title or chapter number.  Different chapters within a document such as a book or a report may each be independently authored, or may all be authored by a single individual or group of authors."@en ;
              rdfs:label "chapter"@en .


###  http://purl.org/spar/fabio/CollectedWorks
fabio:CollectedWorks rdf:type owl:Class ;
                     rdfs:subClassOf fabio:Anthology ;
                     rdfs:comment "A collection of the literary or scholastic works of a single person."@en ;
                     rdfs:label "collected works"@en .


###  http://purl.org/spar/fabio/ComputerFile
fabio:ComputerFile rdf:type owl:Class ;
                   rdfs:subClassOf fabio:DigitalItem ;
                   rdfs:comment "A digital item containing information in computer-readable form encoded in a particular format."@en ;
                   rdfs:label "computer file"@en .


###  http://purl.org/spar/fabio/DigitalItem
fabio:DigitalItem rdf:type owl:Class ;
                  rdfs:subClassOf fabio:Item ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty fabio:isStoredOn ;
                                    owl:someValuesFrom fabio:DigitalStorageMedium
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty frbr:exemplarOf ;
                                    owl:someValuesFrom fabio:DigitalManifestation
                                  ] ;
                  <http://purl.org/dc/elements/1.1/description> "A digital item is an exemplar of a digital manifestation only and it is always stored in a storage medium suitable for digital objects, such as CDs, DVDs, HDs and the Web."@en ;
                  rdfs:comment "A digital object, such as a computer file."@en ;
                  rdfs:label "digital item"@en .


###  http://purl.org/spar/fabio/DigitalManifestation
fabio:DigitalManifestation rdf:type owl:Class ;
                           rdfs:subClassOf fabio:Manifestation ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty frbr:exemplar ;
                                             owl:someValuesFrom fabio:DigitalItem
                                           ] ;
                           rdfs:comment "A manifestation that represents data in binary form, encoding the data as a series of 0s and 1s."@en ;
                           rdfs:label "digital manifestation"@en .


###  http://purl.org/spar/fabio/DigitalStorageMedium
fabio:DigitalStorageMedium rdf:type owl:Class ;
                           rdfs:subClassOf fabio:StorageMedium ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty fabio:stores ;
                                             owl:allValuesFrom fabio:DigitalItem
                                           ] ;
                           rdfs:comment "A means of storing information in digital form, involving binary encoding of data in 0s and 1s, e.g. a computer random access memory, hard disc, USB stick, CD, DVD or digital magnetic tape."@en ;
                           rdfs:label "digital storage medium"@en .


###  http://purl.org/spar/fabio/DustJacket
fabio:DustJacket rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Expression ;
                 rdfs:comment "A detachable outer cover, usually made of paper and printed with text and illustrations. This outer cover has folded flaps that hold it to the cover of a document."@en ;
                 rdfs:label "dust jacket"@en .


###  http://purl.org/spar/fabio/Entry
fabio:Entry rdf:type owl:Class ;
            rdfs:subClassOf fabio:Expression ;
            rdfs:comment "An item written or printed in a diary, list, account book, reference book, or database."@en ;
            rdfs:label "entry"@en .


###  http://purl.org/spar/fabio/Expression
fabio:Expression rdf:type owl:Class ;
                 rdfs:subClassOf frbr:Expression ,
                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty frbr:part ;
                                                          owl:allValuesFrom frbr:Expression
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty frbr:partOf ;
                                                          owl:allValuesFrom frbr:Expression
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty frbr:embodiment ;
                                   owl:allValuesFrom fabio:Manifestation
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty frbr:realizationOf ;
                                   owl:allValuesFrom fabio:Work
                                 ] ;
                 <http://purl.org/dc/elements/1.1/description> "A fabio:Expression can only have part or be part of another fabio:Expression.  Moreover, it can be a representation only of a fabio:Work, and  it can be embodied only in fabio:Manifestation(s)."@en ;
                 rdfs:comment "A subclass of FRBR expression, restricted to expressions of fabio:Works.  For your latest research paper, the preprint submitted to the publisher, and the final published version to which the publisher assigned a unique digital object identifier, are both expressions of the same work.  "@en ;
                 rdfs:label "expression"@en .


###  http://purl.org/spar/fabio/ExpressionCollection
fabio:ExpressionCollection rdf:type owl:Class ;
                           rdfs:subClassOf fabio:Expression ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty frbr:part ;
                                             owl:someValuesFrom fabio:Expression
                                           ] ;
                           rdfs:comment "A collection of expressions, for example a periodical or a book series."@en ;
                           rdfs:label "expression collection"@en .


###  http://purl.org/spar/fabio/InstructionalWork
fabio:InstructionalWork rdf:type owl:Class ;
                        rdfs:subClassOf fabio:Work .


###  http://purl.org/spar/fabio/Item
fabio:Item rdf:type owl:Class ;
           rdfs:subClassOf frbr:Item ,
                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty frbr:part ;
                                                    owl:allValuesFrom frbr:Item
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty frbr:partOf ;
                                                    owl:allValuesFrom frbr:Item
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty frbr:exemplarOf ;
                             owl:allValuesFrom fabio:Manifestation
                           ] ;
           <http://purl.org/dc/elements/1.1/description> "A fabio:Item can only have part or be part of another fabio:Item. Moreover, it can be an exemplar only of a fabio:Manifestation."@en ;
           rdfs:comment "A subclass of FRBR item, restricted to exemplars of fabio:Manifestations.  An example of a fabio:Item is a printed copy of a journal article on your desk, or a PDF file of that article that you purchased from a publisher and that now resides in digital form on your computer hard drive.  "@en ;
           rdfs:label "item"@en .


###  http://purl.org/spar/fabio/ItemCollection
fabio:ItemCollection rdf:type owl:Class ;
                     rdfs:subClassOf fabio:Item ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty frbr:part ;
                                       owl:someValuesFrom fabio:Item
                                     ] ;
                     rdfs:comment "A collection of items."@en ;
                     rdfs:label "item collection"@en .


###  http://purl.org/spar/fabio/Journal
fabio:Journal rdf:type owl:Class ;
              rdfs:subClassOf fabio:Periodical ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty frbr:part ;
                                owl:someValuesFrom fabio:JournalIssue
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty frbr:part ;
                                owl:someValuesFrom fabio:JournalVolume
                              ] ;
              rdfs:comment "A scholarly periodical primarily devoted to the publication of original research papers. [Printed and electronic manifestations of the same journal are usually identified by separate print and electronic International Standard Serial Numbers (ISSN or eISSN, respectively), that identifies the journal as a whole, not to individual issues of it.]"@en ;
              rdfs:label "journal"@en .


###  http://purl.org/spar/fabio/JournalArticle
fabio:JournalArticle rdf:type owl:Class ;
                     rdfs:subClassOf fabio:Article ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty frbr:partOf ;
                                       owl:someValuesFrom fabio:JournalIssue
                                     ] ;
                     rdfs:comment "An article, typically the realization of a research paper reporting original research findings, published in a journal issue.  "@en ;
                     rdfs:label "journal article"@en .


###  http://purl.org/spar/fabio/JournalIssue
fabio:JournalIssue rdf:type owl:Class ;
                   rdfs:subClassOf fabio:PeriodicalIssue ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty frbr:partOf ;
                                     owl:someValuesFrom fabio:JournalVolume
                                   ] ;
                   rdfs:comment "A particular published issue of a journal, one or more of which will constitute a volume of the journal."@en ;
                   rdfs:label "journal issue"@en .


###  http://purl.org/spar/fabio/JournalNewsItem
fabio:JournalNewsItem rdf:type owl:Class ;
                      rdfs:subClassOf fabio:NewsItem ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty frbr:partOf ;
                                        owl:someValuesFrom fabio:JournalIssue
                                      ] ;
                      rdfs:comment "A news report published in a journal issue."@en ;
                      rdfs:label "journal news item"@en .


###  http://purl.org/spar/fabio/JournalVolume
fabio:JournalVolume rdf:type owl:Class ;
                    rdfs:subClassOf fabio:PeriodicalVolume ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:partOf ;
                                      owl:someValuesFrom fabio:Journal
                                    ] ;
                    rdfs:comment "A particular published volume of a journal, comprising one or more journal issues."@en ;
                    rdfs:label "journal volume"@en .


###  http://purl.org/spar/fabio/Letter
fabio:Letter rdf:type owl:Class ;
             rdfs:subClassOf fabio:Expression ;
             rdfs:comment "A written or printed communication of a personal or professional nature between individuals and/or representatives of corporate bodies, usually transmitted by the postal service or published in a periodical.  In the latter case, the letter is typically addressed to the editor and comments on or discussed an item previously published by that periodical, or of interest to its readership."@en ;
             rdfs:label "letter"@en .


###  http://purl.org/spar/fabio/LiteraryArtisticWork
fabio:LiteraryArtisticWork rdf:type owl:Class ;
                           rdfs:subClassOf fabio:ArtisticWork ;
                           rdfs:comment "A literary creative work, such as a novel, play, poem or song."@en ;
                           rdfs:label "literary artistic work"@en .


###  http://purl.org/spar/fabio/Magazine
fabio:Magazine rdf:type owl:Class ;
               rdfs:subClassOf fabio:Periodical ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty frbr:part ;
                                 owl:someValuesFrom fabio:MagazineIssue
                               ] ;
               rdfs:comment "A periodical, usually devoted to a particular topic or domain of interest, and usually published weekly or monthly, consisting primarily of  non-peer reviewed editorials, journalistic news items and more substantive articles, reviews, book reviews and discussions concerning current or recent events and publications, and matters of interest to the domain served by the magazine.  [Some scientific journals, notably Science and Nature, also secondarily serve as science magazines by containing substantive editorials and news items on vital or controversial issues]."@en ;
               rdfs:label "magazine"@en .


###  http://purl.org/spar/fabio/MagazineArticle
fabio:MagazineArticle rdf:type owl:Class ;
                      rdfs:subClassOf fabio:Article ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty frbr:partOf ;
                                        owl:someValuesFrom fabio:MagazineIssue
                                      ] ;
                      rdfs:comment "An article published in a magazine issue."@en ;
                      rdfs:label "magazine article"@en .


###  http://purl.org/spar/fabio/MagazineIssue
fabio:MagazineIssue rdf:type owl:Class ;
                    rdfs:subClassOf fabio:PeriodicalIssue ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:partOf ;
                                      owl:someValuesFrom fabio:Magazine
                                    ] ;
                    rdfs:comment "A particular published  issue of a magazine, identified by date, and sometimes also by place (e.g. 'West Coast edition') or language (e.g. 'Spanish edition')."@en ;
                    rdfs:label "magazine issue"@en .


###  http://purl.org/spar/fabio/MagazineNewsItem
fabio:MagazineNewsItem rdf:type owl:Class ;
                       rdfs:subClassOf fabio:NewsItem ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty frbr:partOf ;
                                         owl:someValuesFrom fabio:MagazineIssue
                                       ] ;
                       rdfs:comment "A news report published in a magazine issue."@en ;
                       rdfs:label "magazine news item"@en .


###  http://purl.org/spar/fabio/Manifestation
fabio:Manifestation rdf:type owl:Class ;
                    rdfs:subClassOf frbr:Manifestation ,
                                    [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty frbr:part ;
                                                             owl:allValuesFrom frbr:Manifestation
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty frbr:partOf ;
                                                             owl:allValuesFrom frbr:Manifestation
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:embodimentOf ;
                                      owl:allValuesFrom fabio:Expression
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:exemplar ;
                                      owl:allValuesFrom fabio:Item
                                    ] ;
                    <http://purl.org/dc/elements/1.1/description> "A fabio:Manifestation can only have part or be part of another fabio:Manifestation.  Moreover, it can be an embodiment only of a fabio:Expression and it can be exemplified only by fabio:Item(s)."@en ;
                    rdfs:comment """A subclass of FRBR manifestation, restricted to manifestations of fabio:Expressions. fabio:Manifestation specifically applies to electronic (digital) as well as to physical manifestations of expressions.  

Examples of different manifestations of a single 'version of record' expression of a scholarly work include an article in a print journal or the on-line version of that article as a web page."""@en ;
                    rdfs:label "manifestation"@en .


###  http://purl.org/spar/fabio/ManifestationCollection
fabio:ManifestationCollection rdf:type owl:Class ;
                              rdfs:subClassOf fabio:Manifestation ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty frbr:part ;
                                                owl:someValuesFrom fabio:Manifestation
                                              ] ;
                              rdfs:comment "A collection of manifestations."@en ;
                              rdfs:label "manifestation collection"@en .


###  http://purl.org/spar/fabio/Manuscript
fabio:Manuscript rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Expression ;
                 rdfs:comment "A textual work prepared 'by hand', such as a typescript or word-processed pre-publication draft of a research paper or a report, or a work not otherwise reproduced in multiple copies.  [Note: fabio:Manuscript is not intended to describe a handwritten historical document on paper or parchment, for which the FRBR distinctions between work, expression, manifestation and item (individual copy) becomes blurred.]."@en ;
                 rdfs:label "manuscript"@en .


###  http://purl.org/spar/fabio/NewsItem
fabio:NewsItem rdf:type owl:Class ;
               rdfs:subClassOf fabio:Expression ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty frbr:realizationOf ;
                                 owl:someValuesFrom fabio:NewsReport
                               ] ;
               rdfs:comment "A published news report."@en ;
               rdfs:label "news item"@en .


###  http://purl.org/spar/fabio/NewsReport
fabio:NewsReport rdf:type owl:Class ;
                 rdfs:subClassOf fabio:Work .


###  http://purl.org/spar/fabio/Newspaper
fabio:Newspaper rdf:type owl:Class ;
                rdfs:subClassOf fabio:Periodical ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty frbr:part ;
                                  owl:someValuesFrom fabio:NewspaperIssue
                                ] ;
                rdfs:comment "A non-peer reviewed periodical, usually published daily or weekly, consisting primarily of editorials and news items concerning current or recent events and matters of public interest."@en ;
                rdfs:label "newspaper"@en .


###  http://purl.org/spar/fabio/NewspaperArticle
fabio:NewspaperArticle rdf:type owl:Class ;
                       rdfs:subClassOf fabio:Article ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty frbr:partOf ;
                                         owl:someValuesFrom fabio:NewspaperIssue
                                       ] ;
                       rdfs:comment "An article written by a journalist and published in a newspaper."@en ;
                       rdfs:label "newspaper article"@en .


